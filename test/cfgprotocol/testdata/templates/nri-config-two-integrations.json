{
  "config_protocol_version": "1",
  "action": "register_config",
  "config_name": "myconfig",
  "config": {
    "integrations": [
      {
        "labels": {
          "timestamp": "{{.timestampLong}}"
        },
        "name": "nri-integration-long",
        "exec": [
          "go",
          "run",
          "testdata/go/spawner.go",
          "-path",
          "testdata/scenarios/shared/nri-out.json",
          "-singleLine",
          "-forever"
        ]
      },
      {
        "labels": {
          "timestamp": "{{.timestampShort}}"
        },
        "name": "nri-integration-short",
        "exec": [
          "go",
          "run",
          "testdata/go/spawner.go",
          "-path",
          "testdata/scenarios/shared/nri-out.json"
        ]
      }
    ]
  }
}