{
  "config_protocol_version": "1",
  "action": "register_config",
  "config_name": "myconfig",
  "config": {
    "integrations": [
      {
        "name": "{{.processName}}",
        "labels": {
          "timestamp": "{{.timestamp}}"
        },
        "exec": [
          "go",
          "run",
          "testdata/go/spawner.go",
          "-path",
          "testdata/scenarios/shared/nri-out.json",
          "-nri-process-name",
          "{{.processName}}",
          "-mode",
          "long"
        ],
        "interval": "2s"
      }
    ]
  }
}